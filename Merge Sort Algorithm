using namespace std;

void merg(int arr[] , int begin , int mid , int end)
{
	int tem1 = mid - begin + 1 ;
	int tem2 = end - mid ;
	int left[tem1] , right[tem2] ;
	for (int i = 0 ; i < tem1 ; i++)
		left[i] = arr[begin + i] ;
	for (int i = 0 ; i < tem2 ; i++)
		right[i] = arr[mid + 1 + i] ;

	int i = 0 , j = 0 , k = begin ;
	while (i < tem1 && j < tem2)
	{
		if (left[i] <= right[j])
		{
			arr[k] = left[i] ;
			i++ ; k++ ;
		}
		else {
			arr[k] = right[j] ;
			j++ ; k++ ;
		}
	}
	for ( ; i < tem1 ; i++) {
		arr[k] = left[i] ;
	}
	for (; j < tem2 ; j++) {
		arr[k] = right[j] ;
	}
	delete[] left ;			delete[] right ;
}


void mergsort(int arr[] , int begin , int end)
{
	if (begin >= end) return ; // terminator .
	int mid = begin + (end - begin) / 2 ;
	mergsort(arr , begin , mid) ;
	mergsort(arr , mid + 1 , end) ;
	merg(arr , begin , mid , end) ;
}


int main()
{
	int n;
	cin >> n ;
	int arr[n] ;
	for (int i = 0 ; i < n; i++)
		cin >> arr[i] ;

	mergsort(arr , 0 , n - 1) ;

	for (auto i = 0; i < n ; i++)
		cout << arr[i] << " ";
}





